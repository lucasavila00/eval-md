{
    "name": "eval-md",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "jest",
        "lint": "eslint .",
        "prettier-check": "prettier --check \"{src,scripts,tests}/**/*.{js,jsx,ts,tsx,json,css,md}\"",
        "static": "npm run prettier-check && npm run ts",
        "exec": "ts-node src/bin.ts",
        "ts": "tsc --noEmit",
        "mon": "nodemon --watch ./eval-mds -e md --exec \"ts-node\" --transpileOnly ./src/bin.ts",
        "mont": "nodemon --watch ./src -e ts --exec \"ts-node\" --transpileOnly ./src/bin.ts",
        "prof": "node -r ts-node/register --inspect-brk ./src/bin.ts ",
        "watch": "EVAL_MD_WATCH=yes NODE_OPTIONS=\"-r ts-node/register\" TS_NODE_TRANSPILE_ONLY=true node ./src/bin.ts",
        "e2e:test1": "EVAL_MD_WILL_THROW=yes EVAL_MD_SRC_DIR=e2e/test1 EVAL_MD_OUT_DIR=e2e/out/test1 NODE_OPTIONS=\"-r ts-node/register\" TS_NODE_TRANSPILE_ONLY=true node ./src/bin.ts > e2e/out/test1/stdout.txt"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@babel/core": "^7.18.6",
        "@babel/generator": "^7.18.7",
        "@babel/parser": "^7.18.6",
        "@babel/preset-typescript": "^7.18.6",
        "@babel/types": "^7.18.7",
        "chokidar": "^3.5.3",
        "fp-ts": "^2.12.1",
        "fs-extra": "^10.1.0",
        "glob": "^8.0.3",
        "io-ts": "^2.2.16",
        "logging-ts": "^0.3.4",
        "parser-ts": "^0.6.16",
        "rimraf": "^3.0.2",
        "ts-morph": "^15.1.0"
    },
    "devDependencies": {
        "@types/fs-extra": "^9.0.13",
        "@types/glob": "^7.2.0",
        "@types/jest": "^28.1.4",
        "@types/node": "^18.0.0",
        "@types/rimraf": "^3.0.2",
        "@typescript-eslint/eslint-plugin": "^5.30.3",
        "@typescript-eslint/parser": "^5.30.3",
        "eslint": "^8.19.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-jest": "^26.5.3",
        "jest": "^28.1.2",
        "nodemon": "^2.0.18",
        "prettier": "^2.7.1",
        "snapshot-diff": "^0.9.0",
        "ts-jest": "^28.0.5",
        "ts-node": "^10.8.1",
        "typescript": "^4.7.4"
    }
}
